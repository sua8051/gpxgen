{
  "compilerOptions": {
    "lib": [
      "dom",
      "es5",
      "es2015",
      "es2015.promise",
      "es2016"
    ],
    "sourceMap": true,
    "jsx": "react",
    "target": "es5",
    "strict": true,
    "module": "es2015",
    "moduleResolution": "node",
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": false,
    "baseUrl": "."
  },
  "exclude": [
    "node_modules/**/*"
  ]
}
